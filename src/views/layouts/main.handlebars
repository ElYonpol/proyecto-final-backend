{{!-- <!DOCTYPE html> --}}
<!DOCTYPE html>
<html lang="es">
	<head>
		<meta charset="UTF-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<meta
			name="description"
			content="Brindamos soluciones integrales en estética dental, blanqueamiento, endodoncia, odontología general, ortodoncia y prótesis."
		/>
		<meta
			name="keywords"
			content="odontología, endodoncia, tratamiento de conducto, ortodoncia, prótesis, blanqueamiento, estética dental, caries, Portas Esquivel"
		/>

		<!-- OPEN GRAPH -->

		<meta property="og:title" content="Portas Esquivel & Asociados" />
		<meta property="og:image" content="https://i.ibb.co/vQXP3xS/icon-pe.jpg" />

		<link rel="shortcut icon" href="/static/assets/icons/icon_pe.jpg" />

		<!-- GOOGLE FONTS -->
		<link
			href="https://fonts.googleapis.com/css2?family=Montserrat:wght@400;700;900&display=swap"
			rel="stylesheet"
		/>

		<!-- FONTAWESOME -->
		<script
			src="https://kit.fontawesome.com/c8592c7dd1.js"
			crossorigin="anonymous"
		></script>

		<!-- CSS only DE BOOTSTRAP CDN -->
		<link
			href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css"
			rel="stylesheet"
			integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx"
			crossorigin="anonymous"
		/>

		<!-- JavaScript Bundle with Popper de BOOTSTRAP -->
		<script
			defer
			src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js"
			integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa"
			crossorigin="anonymous"
		></script>

		<!-- Librería JS Sweet Alert -->
		<script defer src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>

		<!-- Librerías CSS y JS Toastify -->
		<link
			rel="stylesheet"
			type="text/css"
			href="https://cdn.jsdelivr.net/npm/toastify-js/src/toastify.min.css"
		/>
		<script
			defer
			type="text/javascript"
			src="https://cdn.jsdelivr.net/npm/toastify-js"
		></script>

		<link rel="stylesheet" href="/static/css/index.css" />
		<script defer src="/static/js/index.js"></script>
		<script defer src="/static/js/logout.js"></script>

		<title>Portas Esquivel & Asociados | Odontología de Excelencia</title>
	</head>
	<body class="grid--container">
		<header id="header">
			<nav id="bs--navbar">
				<div class="row">
					<div class="logo">
						<a href="/"><img
								class="image"
								src="/static/assets/icons/icon_pe.jpg"
								alt="Logo Portas Esquivel & Asociados"
							/></a>
					</div>
					<div class="col">
						<ul class="nav__list">
							<li>
								<a class="nav__link" href="/">Inicio</a>
							</li>
							<li>
								<a class="nav__link" href="/products">Productos</a>
							</li>
							<li>
								<a class="nav__link" href="/realtimeproducts"> Real-Time Productos</a>
							</li>
							<li>
								<a class="nav__link" href="/carts">Carritos</a>
							</li>
							<li>
								<a class="nav__link" href="/users">Usuarios</a>
							</li>
							<li>
								<a class="nav__link" href="/chat">Chat</a>
							</li>
							<li>
								<a class="nav__link" href="/emails">Emails</a>
							</li>
							<li>
								<a class="nav__link" href="/register">Registro</a>
							</li>
							<li>
								<a class="nav__link" href="/login">Login</a>
							</li>
							<li>
								<form id="logoutForm" action="/api/sessions/logout" method="get"><button class="btn--transparent" type="submit">Logout</button></form>
							</li>
						</ul>
					</div>
				</div>
			</nav>
		</header>
		<section id="main">
			<div class="col-md-4 offset-md-4">
				{{> messages }}
				{{> errors }}
			</div>
			{{{body}}}
		</section>
		<footer id="footer">
			<div class="row--footer">
				<div class="footer__social">Encontranos en las redes</div>
				<div class="footer__social">
					<!-- Íconos Redes Sociales -->
					<a href="#" title="Acceso a enviarnos un correo electrónico"><i
							class="fa-solid fa-square-envelope fa-fontColor"
						></i></a>
					<a href="#" title="Acceso a nuestra página de Twitter"><i
							class="fa-brands fa-twitter-square fa-fontColor"
						></i></a>
					<a href="#" title="Acceso a nuestra página de Instagram"><i
							class="fa-brands fa-instagram-square fa-fontColor"
						></i></a>
					<a href="#" title="Acceso a nuestra página de Facebook"><i
							class="fa-brands fa-facebook-square fa-fontColor"
						></i></a>
				</div>
				<div class="footer__bottom__copyright">
					&copy; Copyright 2023
					<a href="#">Portas Esquivel & Asociados</a>
					| Creada por Juan Pablo Portas Esquivel
				</div>
			</div>
		</footer>
	</body>
</html>